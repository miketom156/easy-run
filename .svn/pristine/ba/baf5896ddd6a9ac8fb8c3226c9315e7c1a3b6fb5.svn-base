package com.job5156.vo.file;

import java.util.List;

/**
 * 数据封装给第三方的邮件内容VO
 */
public class EmailToFilevo {

    private String email;       //收件人email
    private String userName; //收件人
    private Integer dateInterval;     //多少天未登录
    private String unsubscribeLink;  //退订链接
    private String morePosLink;      //更多工作链接
    private String visit_domain;     //http://www.job5156.com
    private String search_domain;    //搜索服务器地址

    private List<Position> positionList; //邮件内容列表

    public static class Position{

        private String posNo;	 // 职位编号
        private String posName;  //职位名称
        private String comId;    //公司ID
        private String comName;  //公司名称
        private String workLocationStr; //工作地区
        private String reqWorkYearStr;  //工作经验
        private String reqDegreeStr;    //学历
        private String refreshDate;     //更新时间

        public String getPosNo() {
            return posNo;
        }

        public void setPosNo(String posNo) {
            this.posNo = posNo;
        }

        public String getPosName() {
            return posName;
        }

        public void setPosName(String posName) {
            this.posName = posName;
        }

        public String getComId() {
            return comId;
        }

        public void setComId(String comId) {
            this.comId = comId;
        }

        public String getComName() {
            return comName;
        }

        public void setComName(String comName) {
            this.comName = comName;
        }

        public String getWorkLocationStr() {
            return workLocationStr;
        }

        public void setWorkLocationStr(String workLocationStr) {
            this.workLocationStr = workLocationStr;
        }

        public String getReqWorkYearStr() {
            return reqWorkYearStr;
        }

        public void setReqWorkYearStr(String reqWorkYearStr) {
            this.reqWorkYearStr = reqWorkYearStr;
        }

        public String getReqDegreeStr() {
            return reqDegreeStr;
        }

        public void setReqDegreeStr(String reqDegreeStr) {
            this.reqDegreeStr = reqDegreeStr;
        }

        public String getRefreshDate() {
            return refreshDate;
        }

        public void setRefreshDate(String refreshDate) {
            this.refreshDate = refreshDate;
        }
    }

    public String getEmail() {
        return email;
    }

    public void setEmail(String email) {
        this.email = email;
    }

    public String getUserName() {
        return userName;
    }

    public void setUserName(String userName) {
        this.userName = userName;
    }

    public Integer getDateInterval() {
        return dateInterval;
    }

    public void setDateInterval(Integer dateInterval) {
        this.dateInterval = dateInterval;
    }

    public String getUnsubscribeLink() {
        return unsubscribeLink;
    }

    public void setUnsubscribeLink(String unsubscribeLink) {
        this.unsubscribeLink = unsubscribeLink;
    }

    public String getMorePosLink() {
        return morePosLink;
    }

    public void setMorePosLink(String morePosLink) {
        this.morePosLink = morePosLink;
    }

    public String getVisit_domain() {
        return visit_domain;
    }

    public void setVisit_domain(String visit_domain) {
        this.visit_domain = visit_domain;
    }

    public String getSearch_domain() {
        return search_domain;
    }

    public void setSearch_domain(String search_domain) {
        this.search_domain = search_domain;
    }

    public List<Position> getPositionList() {
        return positionList;
    }

    public void setPositionList(List<Position> positionList) {
        this.positionList = positionList;
    }
}
