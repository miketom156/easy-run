package com.job5156.vo.com;


/**
 * 职位分数vo
 * 系统推送简历
 * @author zhaoman
 *2015-07-01
 */
public class ComPositionGradeVo {

	/**
	 * 企业id
	 */
	private Integer dcomId; 
	
	/**
	 * 企业id
	 */
	private Integer comId; 
	/**
	 * 职位id
	 */
	private Integer posId;
	/**
	 * 企业职位一周内收到的简历数量
	 */
	private Integer posCount;
	/**
	 * 收到简历数量的分数
	 */
	private Integer posCountGrade;
	/**
	 * 职位收到的后台推荐的已读简历
	 */
	private Integer readResume;
	/**
	 * 职位收到的后台推荐的所有简历，包括已读和未读
	 */
	private Integer allResume;
	/**
	 * 职位收到的后台推荐的已读简历与职位收到的后台推荐的所有简历比例
	 */
	private double ratio;
	/**
	 * 职位收到的后台推荐的已读简历与职位收到的后台推荐的所有简历比例分数
	 */
	private Integer ratioGrade;
	/**
	 * 总分数
	 */
	private Integer totalGrade;
	
	public Integer getDcomId() {
		return dcomId;
	}
	public void setDcomId(Integer dcomId) {
		this.dcomId = dcomId;
	}
	public Integer getComId() {
		return comId;
	}
	public void setComId(Integer comId) {
		this.comId = comId;
	}
	public Integer getPosId() {
		return posId;
	}
	public void setPosId(Integer posId) {
		this.posId = posId;
	}
	public Integer getReadResume() {
		return readResume;
	}
	public void setReadResume(Integer readResume) {
		this.readResume = readResume;
	}
	public Integer getAllResume() {
		return allResume;
	}
	public void setAllResume(Integer allResume) {
		this.allResume = allResume;
	}
	public Integer getRatioGrade() {
		return ratioGrade;
	}
	public void setRatioGrade(Integer ratioGrade) {
		this.ratioGrade = ratioGrade;
	}
	public Integer getPosCountGrade() {
		return posCountGrade;
	}
	public void setPosCountGrade(Integer posCountGrade) {
		this.posCountGrade = posCountGrade;
	}
	public Integer getPosCount() {
		return posCount;
	}
	public void setPosCount(Integer posCount) {
		this.posCount = posCount;
	}
	public double getRatio() {
		return ratio;
	}
	public void setRatio(double ratio) {
		this.ratio = ratio;
	}
	public Integer getTotalGrade() {
		return totalGrade;
	}
	public void setTotalGrade(Integer totalGrade) {
		this.totalGrade = totalGrade;
	}
	
	
}
